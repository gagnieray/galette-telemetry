{
  "php-core-extensions": [
    "core",
    "standard",
    "spl",
    "json",
    "pcre",
    "session",
    "mbstring",
    "random"
  ],
  "symbol-whitelist": [
    "Twig\\Extension\\DebugExtension",
    "Twig\\Extension\\AbstractExtension",
    "Twig\\Extension\\GlobalsInterface",
    "Illuminate\\Support\\Facades\\DB",

    "// DI functions",
    "DI\\ContainerBuilder",

    "// Not detected, do not know why.",
    "Psr\\Container\\ContainerInterface"
  ],
  "scan-files": [
    "*.php",
    "app/**/.php",
    "public/*.php"
  ]
}
